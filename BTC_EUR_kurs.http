<script language="javascript" type="text/javascript">$(document).ready(function() {
        $.getJSON('/json/chart/stats_hourly_btceur_statistics_48hour.json', function(data) {

        // split the data set into ohlc and volume
		var ohlc = [],
			mid_3h = [];
			mid_12h = [];
    		dataLength = data.length;



		for (i = 0; i < dataLength; i++) {
		    if(
                data[i][0] &&
                data[i][1] > 0 &&
                data[i][2] > 0 &&
                data[i][3] > 0 &&
                data[i][4] > 0 &&
                data[i][7] > 0 &&
                data[i][8] > 0
            )
            {
                ohlc.push([
                        data[i][0], // the date
                        data[i][1], // open
                        data[i][2], // high
                        data[i][3], // low
                        data[i][4] // close
                ]);


                if(data[i][7] > 0){
                    mid_3h.push([
                        data[i][0],
                        data[i][7]
                    ]);
                }

                if(data[i][8] > 0){
                    mid_12h.push([
                        data[i][0],
                        data[i][8]
                    ]);
                }
            }

		}

